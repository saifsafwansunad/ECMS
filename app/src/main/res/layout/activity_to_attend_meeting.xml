<?xml version="1.0" encoding="utf-8"?>
<LinearLayout class="androidx.appcompat.app.AlertController$RecycleListView" xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:id="@+id/main"
    android:orientation="vertical"
    android:layout_height="match_parent"
    tools:context=".ToAttendMeetingActivity">

    <RelativeLayout
        android:id="@+id/relative1"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    <include
        layout="@layout/toolbar"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="No Meetings for you"
        android:id="@+id/no_Meetings_tv"
        android:layout_centerInParent="true"
        android:layout_gravity="center"
        android:textSize="20dp"
        android:visibility="visible"
        android:textColor="@color/colorBlack"
        android:textStyle="italic|bold"/>






      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="wrap_content"
          android:layout_marginTop="50dp">

          <androidx.recyclerview.widget.RecyclerView

              android:visibility="gone"
              android:layout_width="match_parent"
              android:layout_height="wrap_content"
              android:layout_marginTop="10dp"
              android:id="@+id/to_attend_meetings_recyclerview"/>
      </RelativeLayout>




</RelativeLayout>
</LinearLayout>